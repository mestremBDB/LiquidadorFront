package core.rsolano.liqinterface.model.graph.model;

public class ChargeByPeriod {
    
    private String Color;
    private String Serie;
    private String Valor;
    private int estadotransaccion;
    private int totaltransacciones;
    private double totalcobro;

    public ChargeByPeriod(){
    }
    
    public ChargeByPeriod(int estadotransaccion, int totaltransacciones, double totalcobro, 
        String Serie, String Color, String Valor ){
        this.estadotransaccion = estadotransaccion;
        this.totaltransacciones = totaltransacciones;
        this.totalcobro = totalcobro;
        this.Serie = Serie;
        this.Color = Color;
        this.Valor = Valor;
    }
    
    public void setEstadotransaccion(int estadotransaccion) {
        this.estadotransaccion = estadotransaccion;
    }

    public int getEstadotransaccion() {
        return estadotransaccion;
    }

    public void setTotaltransacciones(int totaltransacciones) {
        this.totaltransacciones = totaltransacciones;
    }

    public int getTotaltransacciones() {
        return totaltransacciones;
    }

    public void setTotalcobro(double totalcobro) {
        this.totalcobro = totalcobro;
    }

    public double getTotalcobro() {
        return totalcobro;
    }
    public void setColor(String Color) {
        this.Color = Color;
    }

    public String getColor() {
        return Color;
    }

    public void setSerie(String Serie) {
        this.Serie = Serie;
    }

    public String getSerie() {
        return Serie;
    }
    
    public void setValor(String Valor) {
        this.Valor = Valor;
    }

    public String getValor() {
        return Valor;
    }
}
